  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/bin/nestjs-protobuf-compiler.js",
      "args": [
        "-p",
        "${cwd}/test/proto",
        "-o",
        "${cwd}/test/compiled",
        "--verbose"
      ],
      "preLaunchTask": "npm: build:dev",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
